{
  "name": "accursed",
  "version": "0.0.3",
  "description": "blessed contributions, extensions, typings and documentation",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "test-debug": "node --inspect-brk node_modules/ts-node/dist/bin node_modules/jasmine/bin/jasmine.js",
    "prepare": "tsc",
    "prettier": "npx ts-refactor organizeImports \"src/**/*.ts*\" \"spec/**/*.ts*\" --dontAsk && npx prettier  --write --arrow-parens avoid --parser typescript --use-tabs false --no-semi true --single-quote true --tab-width 2 --trailing-comma none --end-of-line lf --prose-wrap preserve --print-width 120  --jsx-bracket-same-line true \"src/**/*.ts*\" \"spec/**/*.ts*\"",
    "clean": "rm -rf dist",    
    "doc": "npx typedoc 'src/index.ts' --theme markdown --out api",
    "all": "npm run clean && npm run prettier && npm run build && npm test"
  },
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "@phenomnomnominal/tsquery": "^3.0.0",
    "ansi-escape-sequences": "^4.1.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.12",
    "blessed-xterm": "^1.1.20",
    "drawille": "^1.1.0",
    "drawille-canvas": "^1.2.0",
    "misc-utils-of-mine-generic": "0.0.14",
    "node-keys": "^0.1.5",
    "performance-now": "^2.1.0",
    "shelljs": "^0.8.2",
    "ts-morph": "^1.3.3",
    "ts-simple-ast-extra": "^0.1.4",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "@types/ansi-escape-sequences": "^4.0.0",
    "@types/blessed": "^0.1.11",
    "@types/jasmine": "^2.8.16",
    "@types/node": "^10.5.2",
    "@types/shelljs": "^0.8.4",
    "cli-driver": "0.2.0",
    "jasmine": "^3.3.1",
    "javascript-stringify": "^2.0.0",
    "prettier": "^1.16.4",
    "ts-node": "^7.0.1",
    "ts-refactor": "0.0.2",
    "typescript": "^3.4.1",
    "watch": "^1.0.2"
  },
  "keywords": [
    "TypeScript",
    "refactor",
    "command line",
    "interactive"
  ],
  "repository": "https://github.com/cancerberoSgx/accursed"
}
