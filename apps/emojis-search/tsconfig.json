{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2018",
    "module": "commonjs",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "rootDir": ".",
    "declaration": true,
    "strict": true,
    "jsx": "React",
    "skipLibCheck": true
  },
  "include": [
    "src", "spec"
  ], 
  "exclude": [
    // "node_modules", "test", 
    "src/data/generated", "src/data/scripts"
  ]
}
